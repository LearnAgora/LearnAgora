<!DOCTYPE html>
<html class="full-background" {% block html_tag '' %}  ng-app="learnodex">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
    <meta name="robots" content="noindex,nofollow" />
    <meta name="viewport" content="width=device-width, maximum-scale=1.0" />

    <title>{% block title 'Learnodex' %}</title>

    <link href="{{ asset('bundles/framework/css/structure.css') }}" rel="stylesheet" type="text/css" media="all" />
    <link href="{{ asset('bundles/framework/css/body.css') }}" rel="stylesheet" type="text/css" media="all" />
    <link href="{{ asset('bundles/lalearnodex/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" media="all" />
    <link href="{{ asset('bundles/lalearnodex/css/bootstrap-theme.min.css') }}" rel="stylesheet" type="text/css" media="all" />
    <link href="{{ asset('bundles/lalearnodex/css/sweet-alert.css') }}" rel="stylesheet" type="text/css" media="all" />
    <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />

    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <script type="text/javascript" src="/bower_components/angular/angular.min.js"></script>
    <script type="text/javascript" src="/bower_components/angular-http-loader/app/package/js/angular-http-loader.min.js"></script>

    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

    {% block head %}{% endblock %}

    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', '{{ google_analytics_code }}', 'auto');
        ga('send', 'pageview');
    </script>
</head>

<body {% block body_tags '' %}>
    <script type="text/javascript" src="{{ asset('bundles/lalearnodex/js/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/lalearnodex/js/sweet-alert.min.js') }}"></script>

    <div class="{% block container_class 'container' %}" role="main">
        {% block container %}{% endblock %}
    </div>
</body>

{% block scripts %}{% endblock %}

</html>
